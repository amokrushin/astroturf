{
  "name": "@amokrushin/astroturf",
  "version": "0.9.1",
  "description": "Extract and process inline CSS literals in JavaScript files",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "yarn test:ts && jest",
    "test:ts": "tslint -p ./types",
    "tdd": "jest --watch",
    "prepublishOnly": "yarn build",
    "build": "4c build && cpy types/*.d.ts lib",
    "release": "4c release",
    "start": "yarn --cwd ./example start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/astroturf.git"
  },
  "types": "lib/index.d.ts",
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/astroturf/issues"
  },
  "homepage": "https://github.com/4Catalyzer/astroturf#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "yarn 4c format",
      "git add"
    ],
    "README.md": [
      "doctoc",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "release": {
    "publishDir": "lib"
  },
  "peerDependencies": {
    "webpack": ">=2"
  },
  "devDependencies": {
    "@4c/babel-preset-4catalyzer": "^4.1.1",
    "@4c/cli": "^0.7.1",
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@types/react": "^16.8.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cpy-cli": "^2.0.0",
    "css-loader": "^2.1.0",
    "doctoc": "^1.4.0",
    "dtslint": "^0.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-4catalyzer-react": "^0.9.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "recompose": "^0.30.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "typescript": "next",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/generator": "^7.3.4",
    "@babel/template": "^7.2.2",
    "@babel/types": "^7.3.4",
    "common-tags": "^1.8.0",
    "errno": "^0.1.7",
    "fs-extra": "^7.0.1",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "memory-fs": "^0.4.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
